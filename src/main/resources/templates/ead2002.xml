<?xml version="1.0" encoding="UTF-8"?>
<ead
        xmlns="urn:isbn:1-931666-22-9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:isbn:1-931666-22-9 ../schemas/ead2002.xsd">
    <eadheader>
        <eadid>
        </eadid>
        <filedesc>
            <titlestmt>
                <titleproper>{{title}}</titleproper>
            </titlestmt>
        </filedesc>

        <profiledesc>
            <creation>
                Generated by RODA-in version 2.0
                <date normal="{{now}}">{{now}}</date>
            </creation>
        </profiledesc>

    </eadheader>
    <archdesc level="{{level}}">
        <did>
            <unittitle>{{title}}</unittitle>
            <unitid>{{id}}</unitid>
            <unitdate normal="{{dateInitial}}/{{dateFinal}}">{{dateInitial}}/{{dateFinal}}</unitdate>
            <origination label="producer">{{producer}}</origination>
            <origination label="creator">{{creator}}</origination>
            <langmaterial>{{language}}</langmaterial>
        </did>
        <scopecontent>
            <p>{{description}}</p>
        </scopecontent>
        <userestrict>
            <p>{{rights}}</p>
        </userestrict>
    </archdesc>
</ead>
