<?xml version="1.0" encoding="UTF-8"?>
<ead
        xmlns="urn:isbn:1-931666-22-9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:isbn:1-931666-22-9 ../schemas/ead2002.xsd">
    <eadheader>
        <eadid>
        </eadid>
        <filedesc>
            <titlestmt>
                <titleproper>{{title order=1}}</titleproper>
            </titlestmt>
        </filedesc>

        <profiledesc>
            <creation>
                Generated by RODA-in version 2.0
                <date normal="{{now type='date'}}">{{now}}</date>
            </creation>
        </profiledesc>

    </eadheader>
    <archdesc level="{{level type='list' list='[fonds,subfonds,series,subseries,file,item]'}}">
        <did>
            <unittitle>{{title}}</unittitle>
            <unitid>{{id}}</unitid>
            {{#if dateInitial}}
            {{#if dateFinal}}
            <unitdate normal="{{dateInitial type='date'}}/{{dateFinal type='date'}}">{{dateInitial}}/{{dateFinal}}
            </unitdate>
            {{else}}
            <unitdate normal="{{dateInitial}}">{{dateInitial}}</unitdate>
            {{/if}}
            {{else}}
            {{#if dateFinal}}
            <unitdate normal="{{dateFinal}}">{{dateFinal}}</unitdate>
            {{/if}}
            {{/if}}
            <origination label="producer">{{producer}}</origination>
            <origination label="creator">{{creator}}</origination>
            <langmaterial>{{language}}</langmaterial>
        </did>
        <scopecontent>
            <p>{{description type='textarea'}}</p>
        </scopecontent>
        <userestrict>
            <p>{{rights}}</p>
        </userestrict>
    </archdesc>
</ead>
