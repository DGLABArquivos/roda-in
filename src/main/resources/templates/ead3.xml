<?xml version="1.0" encoding="UTF-8"?>
<ead xmlns="http://ead3.archivists.org/schema/"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://ead3.archivists.org/schema/ ../schemas/ead3.xsd">
    <control>
        <recordid>{{id}}</recordid>
        <filedesc>
            <titlestmt>
                <titleproper>{{title}}</titleproper>
            </titlestmt>
        </filedesc>
        <maintenancestatus value="revised">
        </maintenancestatus>
        <maintenanceagency>
            <agencyname></agencyname>
        </maintenanceagency>
        <maintenancehistory>
            <maintenanceevent>
                <eventtype value="created"></eventtype>
                <eventdatetime>
                </eventdatetime>
                <agenttype value="human">
                </agenttype>
                <agent></agent>
            </maintenanceevent>
        </maintenancehistory>
    </control>
    <archdesc level="class">
        <did>
            <materialspec/>
        </did>
    </archdesc>
</ead>
